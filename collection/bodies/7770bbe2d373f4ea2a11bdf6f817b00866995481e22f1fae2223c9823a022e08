$(document).ready(function () {
    if(window.location.href.indexOf("libraries") > -1 || window.location.href.indexOf("library") > -1 ) {  
	   var libSocialMedia = document.querySelector("#lib-social-media");
	   libSocialMedia.classList.remove("libraries-social-media-hide");
	   libSocialMedia.classList.add("libraries-social-media-show");
    }	
});
</script>

