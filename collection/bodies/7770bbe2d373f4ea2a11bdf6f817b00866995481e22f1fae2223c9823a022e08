$(document).ready(function () {
    if(window.location.href.indexOf("dataprotection") > -1 || window.location.href.indexOf("20117/data_protection") > -1 ) {  
	   var dataProtectionNav = document.querySelector("#dp-nav");
	   dataProtectionNav.classList.remove("dp-nav-hide");
	   dataProtectionNav.classList.add("dp-nav-show");
	   $("#columnPrimary div").css("margin-bottom", "0px");
	   
    }	
});
</script>

